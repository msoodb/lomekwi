
server {
        server_name  monavis.me www.monavis.me;
        root         /var/www/monavis.me;

        error_page 404 /404.html;
            location = /40x.html {
        }

        error_page 500 502 503 504 /50x.html;
            location = /50x.html {
        }


    listen 443 ssl; # managed by Certbot
    ssl_certificate /etc/letsencrypt/live/monavis.me/fullchain.pem; # managed by Certbot
    ssl_certificate_key /etc/letsencrypt/live/monavis.me/privkey.pem; # managed by Certbot
    include /etc/letsencrypt/options-ssl-nginx.conf; # managed by Certbot
    ssl_dhparam /etc/letsencrypt/ssl-dhparams.pem; # managed by Certbot








}
server {
    if ($host = www.monavis.me) {
        return 301 https://$host$request_uri;
    } # managed by Certbot


    if ($host = monavis.me) {
        return 301 https://$host$request_uri;
    } # managed by Certbot


        server_name  monavis.me www.monavis.me;
    listen 80;
    return 404; # managed by Certbot




}